<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.calculator"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/calculator"
        android:label="@string/app_name"
        android:theme="@style/Theme.Sherlock.Light" >
        <activity
            android:name="com.cabot.calculator.activities.CalculatorActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.cabot.calculator.activities.LogDisplayActivity"
            android:label="@string/title_activity_log_display" >
        </activity>
        <activity
            android:name="com.cabot.calculator.activities.LogDisplayActivity"
            android:label="@string/title_activity_log__display_" 
            android:icon="@drawable/log_pad">
       
        </activity>
        
         
    </application>

</manifest>